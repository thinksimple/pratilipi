<datastore-indexes autoGenerate="false">

  <!-- Required by ClaymusService.savePage -->
  <datastore-index kind="PAGE" ancestor="false" source="manual">
    <property name="URI" direction="asc"/>
    <property name="CREATION_DATE" direction="asc"/>
  </datastore-index>

  
  <!-- Required by ClaymusService.getBlogPostContentList -->
  <datastore-index kind="PAGE_CONTENT" ancestor="false" source="manual">
    <property name="X_COL_2" direction="asc"/>
    <property name="_TYPE" direction="asc"/>
    <property name="CREATION_DATE" direction="desc"/>
  </datastore-index>
    
    
  <!-- Required by PratilipiService.getPratilipiList -->
  <datastore-index kind="PRATILIPI" ancestor="false" source="manual">
    <property name="PRATILIPI_TYPE" direction="asc"/>
    <property name="TITLE" direction="asc"/>
  </datastore-index>
    
  <datastore-index kind="PRATILIPI" ancestor="false" source="manual">
    <property name="PRATILIPI_TYPE" direction="asc"/>
    <property name="PUBLIC_DOMAIN" direction="asc"/>
    <property name="TITLE" direction="asc"/>
  </datastore-index>

  <datastore-index kind="PRATILIPI" ancestor="false" source="manual">
    <property name="PRATILIPI_TYPE" direction="asc"/>
    <property name="LANGUAGE_ID" direction="asc"/>
    <property name="TITLE" direction="asc"/>
  </datastore-index>
    
  <datastore-index kind="PRATILIPI" ancestor="false" source="manual">
    <property name="PRATILIPI_TYPE" direction="asc"/>
    <property name="AUTHOR_ID" direction="asc"/>
    <property name="TITLE" direction="asc"/>
  </datastore-index>

    
  <!-- Required by PratilipiService.getAuthorList -->
  <datastore-index kind="AUTHOR" ancestor="false" source="manual">
    <property name="FIRST_NAME_EN" direction="asc"/>
    <property name="LAST_NAME_EN" direction="asc"/>
    <property name="PEN_NAME_EN" direction="asc"/>
  </datastore-index>

</datastore-indexes>
